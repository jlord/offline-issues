<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <link rel="stylesheet" type="text/css" href="css/style.css">
    <title>All Issues</title>
  </head>
  <body>
    <div class="container">
      <header>
        <h1>All Issues</h1>
      </header>
      <ul class="issues">
        {{#issues}}
        <li>
          <div class="meta">
            <a href="filename.html??">{{quicklink}}</a>
            <a class="person" href="{{user.html_url}}">
              {{user.login}}
            </a>
          </div>
        </li>
        {{/issues}}
      </ul>

  </body>
</html>
